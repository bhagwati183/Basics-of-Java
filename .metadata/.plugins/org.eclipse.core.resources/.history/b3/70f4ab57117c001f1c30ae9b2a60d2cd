package Day_1;

public class MethodOverloading {
	
	static int add(int a, int b) {
		return a+b;
	}
	
	static int add(int a , int b, int c) {
		return a+b+c;
	}
	
	static int add(int a, int b , int c, int d) {
		return a+b+c+d;
	}

	public static void main(String[] args) {
		
		add(10,20);
		add(10,20,30);
		add(10,20,30,40);

	}

}
