package Day_1;

import java.util.Scanner;

public class MethodsWithReturnType {
	
	//1.Write a method that takes an integer array and returns the sum of all elements.
	
	public int sumOfArray(int numbers[] ) {
		
		int sum =0;
		for(int i=0; i<numbers.length;i++) {
			sum = sum+numbers[i];
		}
	return sum;
	}
		

	public static void main(String[] args) {
		
		int numbers[] = {1,2,25,23,25};
		
		MethodsWithReturnType arr = new MethodsWithReturnType();
		int arr_result = arr.sumOfArray(numbers);
		System.out.println(arr_result);
		
		
		
	}

}
