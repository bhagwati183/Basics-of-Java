package Day_1;

public class Method_PracticeSheet {
	
	//Create a method that prints "Hello, World!".
	
	static void display() {
		
		System.out.println("Hello World!!!");
		
	}
// Write a method that takes no arguments and prints your name
	static void name() {
		System.out.println("Hi I am Bhagwati");
	}
	public static void main(String[] args) {
		
		display();
		name();
	}

}
